{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@site/*": ["./*"]
    }
  },
  "include": ["src/**/*", "docs/**/*", "blog/**/*", "docusaurus.config.ts", "sidebars.ts"],
  "exclude": [".docusaurus", "build", "node_modules"]
}
 